    "quality_policy.banner.heading": { en: "Quality and Policy", fr: "Qualité et Politique" },
    "quality_policy.breadcrumb.about": { en: "About", fr: "À Propos" },
    "quality_policy.breadcrumb.current": { en: "Quality and Policy", fr: "Qualité et Politique" },
    "quality_policy.breadcrumb.home": { en: "Home", fr: "Accueil" },
    "quality_policy.commitment1": { en: "Reliable Service: Offer inspection and certification services in a reliable, transparent, safe and professional manner.", fr: "Service Fiable : Offrir des services d'inspection et de certification de manière fiable, transparente, sûre et professionnelle." },
    "quality_policy.commitment2": { en: "ISO/IEC 17020 Compliance: Meet the requirements of both customers and applicable legal and regulatory requirements.", fr: "Conformité ISO/IEC 17020 : Répondre aux exigences des clients et aux exigences légales et réglementaires applicables." },
    "quality_policy.commitment3": { en: "Impartial Performance: Ensure IMPARTIAL and INDEPENDENT performance of the Organization and its members.", fr: "Performance Impartiale : Assurer une performance IMPARTIALE et INDÉPENDANTE de l'Organisation et de ses membres." },
    "quality_policy.commitment4": { en: "Competent Staff: Keep inspection staff technically TRAINED and COMPETENT to carry out their activities.", fr: "Personnel Compétent : Maintenir le personnel d'inspection techniquement FORMÉ et COMPÉTENT pour exercer ses activités." },
    "quality_policy.commitment5": { en: "Confidentiality: Make CONFIDENTIAL use of all information obtained and generated during the process.", fr: "Confidentialité : Faire usage CONFIDENTIEL de toutes les informations obtenues et générées pendant le processus." },
    "quality_policy.commitment6": { en: "Continuous Improvement: Continuously improve our management system to consolidate as a reference for quality services.", fr: "Amélioration Continue : Améliorer continuellement notre système de gestion pour nous consolider comme référence de services de qualité." },
    "quality_policy.commitment_intro": { en: "The Senior Management of", fr: "La Direction de" },
