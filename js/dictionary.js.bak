    "quality_policy.quality_standards.heading": { en: "Our Quality Standards", fr: "Nos Normes de Qualité" },
    "quality_policy.quality_standards.iso_certified.description": { en: "Our operations are fully compliant with international inspection standards, ensuring reliability and credibility in all our services.", fr: "Nos opérations sont entièrement conformes aux normes d'inspection internationales, garantissant fiabilité et crédibilité dans tous nos services." },
    "quality_policy.quality_standards.iso_certified.title": { en: "ISO/IEC 17020 Certified", fr: "Certifié ISO/IEC 17020" },
    "quality_policy.quality_standards.technical_excellence.description": { en: "Our team of qualified professionals brings extensive technical expertise to every inspection and certification project.", fr: "Notre équipe de professionnels qualifiés apporte une expertise technique approfondie à chaque projet d'inspection et de certification." },
