    // Quality Policy Page - Complete texts
    "quality_policy.commitment_intro_full": { en: "The Senior Management of GROUP AGECOSCO is committed to:", fr: "La Direction de GROUP AGECOSCO s'engage à :" },
    "quality_policy.commitment1_full": { en: "Reliable Service: Offer inspection and certification services in a reliable, transparent, safe and professional manner.", fr: "Service Fiable : Offrir des services d'inspection et de certification de manière fiable, transparente, sûre et professionnelle." },
    "quality_policy.commitment2_full": { en: "ISO/IEC 17020 Compliance: Meet the requirements of both customers and applicable legal and regulatory requirements.", fr: "Conformité ISO/IEC 17020 : Répondre aux exigences des clients et aux exigences légales et réglementaires applicables." },
    "quality_policy.commitment3_full": { en: "Impartial Performance: Ensure IMPARTIAL and INDEPENDENT performance of the Organization and its members.", fr: "Performance Impartiale : Assurer une performance IMPARTIALE et INDÉPENDANTE de l'Organisation et de ses membres." },
    "quality_policy.commitment4_full": { en: "Competent Staff: Keep inspection staff technically TRAINED and COMPETENT to carry out their activities.", fr: "Personnel Compétent : Maintenir le personnel d'inspection techniquement FORMÉ et COMPÉTENT pour exercer ses activités." },
    "quality_policy.commitment5_full": { en: "Confidentiality: Make CONFIDENTIAL use of all information obtained and generated during the process.", fr: "Confidentialité : Faire usage CONFIDENTIEL de toutes les informations obtenues et générées pendant le processus." },
    "quality_policy.commitment6_full": { en: "Continuous Improvement: Continuously improve our management system to consolidate as a reference for quality services.", fr: "Amélioration Continue : Améliorer continuellement notre système de gestion pour nous consolider comme référence de services de qualité." },
    "quality_policy.quality_management.paragraph1_full": { en: "GROUP AGECOSCO has established a comprehensive Quality Management System to ensure consistent delivery of high-standard inspection and certification services across all our operations in West Africa.", fr: "GROUP AGECOSCO a établi un Système de Management de la Qualité complet pour assurer une prestation cohérente de services d'inspection et de certification de haut niveau dans toutes nos opérations en Afrique de l'Ouest." },
    "quality_policy.quality_management.paragraph2_full": { en: "Our quality framework is built on the foundation of ISO/IEC 17020 standards, ensuring that every aspect of our service delivery meets international best practices and regulatory requirements.", fr: "Notre cadre qualité est construit sur la base des normes ISO/IEC 17020, garantissant que chaque aspect de notre prestation de services respecte les meilleures pratiques internationales et les exigences réglementaires." },
    "quality_policy.quality_management.feature1_full": { en: "Standardized inspection procedures", fr: "Procédures d'inspection standardisées" },
    "quality_policy.quality_management.feature2_full": { en: "Qualified and certified personnel", fr: "Personnel qualifié et certifié" },
    "quality_policy.quality_management.feature3_full": { en: "Regular quality audits and reviews", fr: "Audits et revues qualité réguliers" },
    "quality_policy.quality_management.feature4_full": { en: "Continuous improvement processes", fr: "Processus d'amélioration continue" },
    "quality_policy.quality_standards.description_full": { en: "Committed to excellence in every aspect of our service delivery", fr: "Engagés à l'excellence dans chaque aspect de notre prestation de services" },
    "quality_policy.quality_standards.iso_certified.description_full": { en: "Our operations are fully compliant with international inspection standards, ensuring reliability and credibility in all our services.", fr: "Nos opérations sont entièrement conformes aux normes d'inspection internationales, garantissant fiabilité et crédibilité dans tous nos services." },
    "quality_policy.quality_standards.technical_excellence.description_full": { en: "Our team of qualified professionals brings extensive technical expertise to every inspection and certification project.", fr: "Notre équipe de professionnels qualifiés apporte une expertise technique approfondie à chaque projet d'inspection et de certification." },
    "quality_policy.quality_standards.client_satisfaction.description_full": { en: "We prioritize client needs and maintain transparent communication throughout the entire service delivery process.", fr: "Nous priorisons les besoins des clients et maintenons une communication transparente tout au long du processus de prestation de services." },
    // Health & Safety Page - Complete texts
    "health_safety.hse_approach_heading_full": { en: "Our Health, Safety & Environmental Approach", fr: "Notre Approche Santé, Sécurité et Environnement" },
    "health_safety.hse_approach_paragraph_full": { en: "Group Agecosco Maritime Company adopts a systematic approach to health, safety and environmental management, designed to ensure compliance with legislation and continuous improvement in performance.", fr: "Group Agecosco Maritime Company adopte une approche systématique de la gestion de la santé, de la sécurité et de l'environnement, conçue pour assurer la conformité à la législation et l'amélioration continue des performances." },
    "health_safety.commitment1_full": { en: "Protect staff, customers, suppliers and third parties from health hazards and risks", fr: "Protéger le personnel, les clients, les fournisseurs et les tiers contre les dangers et les risques pour la santé" },
    "health_safety.commitment2_full": { en: "Promote awareness of safe practices, believing all accidents are preventable", fr: "Promouvoir la sensibilisation aux pratiques sûres, croyant que tous les accidents sont évitables" },
    "health_safety.commitment3_full": { en: "Promote individual responsibility for accident prevention", fr: "Promouvoir la responsabilité individuelle pour la prévention des accidents" },
    "health_safety.commitment4_full": { en: "Analyze accidents and incidents with significant consequences", fr: "Analyser les accidents et incidents ayant des conséquences importantes" },
    "health_safety.commitment5_full": { en: "Investigate and report all HSE incidents and near-misses", fr: "Enquêter et signaler tous les incidents HSE et quasi-accidents" },
    "health_safety.commitment6_full": { en: "Set improvement targets with continuous evaluation and reporting", fr: "Fixer des objectifs d'amélioration avec évaluation et rapports continus" },
    "health_safety.stat1_desc_full": { en: "GROUP AGECOSCO was created in 1998", fr: "GROUP AGECOSCO a été créé en 1998" },
